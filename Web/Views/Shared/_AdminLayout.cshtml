<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
    <link href="~/Scripts/Stylo.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
    <title>HealthCare</title>
</head>
<body>
    <div class="alert alert-success" style="width: 100%; height: 60px; margin: 0"></div>
    <a href="/"><div class="jumbotron page-header" style="margin: 0"></div></a>
    <div class="alert alert-info" style="width:100%; height: 60px">
        <a class="btn btn-info" href="/Login/Desconectar">Desconectar</a>
        <a href="/Empresas/Pedidos" class="btn btn-success navbar-right">Ver Pedidos</a>
        <a class="btn btn-info navbar-right" href="/Login/EditarCuenta" style="margin-right: 20px">Editar Cuenta</a>
    </div>
    <div>
        @if (TempData["Mensaje"] != null)
        {
            <div class="alert alert-success">@TempData["Mensaje"]</div>
        }
    </div>
    <div class="container" id="body">
        @RenderBody()
    </div> 
    <div id="bottom" class="alert alert-success">
    </div>  
</body>
</html>

